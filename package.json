{
  "name": "vscode-charactercount",
  "publisher": "hogashi",
  "displayName": "vscode-charactercount",
  "description": "counts characters in active file",
  "version": "0.0.2",
  "repository": "https://github.com/hogashi/vscode-charactercount",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "8.0.0",
    "@types/mocha": "10.0.0",
    "@types/node": "16.18.0",
    "@types/vscode": "1.72.0",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "@vscode/test-electron": "2.2.0",
    "eslint": "8.26.0",
    "eslint-config-standard-with-typescript": "23.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.3.0",
    "eslint-plugin-promise": "6.1.1",
    "glob": "8.0.3",
    "mocha": "10.1.0",
    "typescript": "4.8.4"
  }
}
